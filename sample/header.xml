<?xml version="1.0" encoding="UTF-8" ?>
<register>
    <header>
        <!-- Header side, all register configuration information's.  -->
        <version>1.0</version>
        <!-- Register path, automatic update on API use. -->
        <rights>
            <!-- Rights administration, will be apply to all variables by default. -->
            <read>true</read>
            <!-- For reading. -->
            <write>true</write>
            <!-- For writing. -->
            <open>true</open>
            <!-- For opening, not mandatory, true by default. -->
            <copy>true</copy>
            <!-- For copying, not mandatory, true by default. -->
            <move>true</move>
            <!-- For moving, not mandatory, true by default. -->
            <destroy>true</destroy>
            <!-- For destroying, not mandatory, true by default. -->
        </rights>
    </header>
    <body>
    </body>
</register>